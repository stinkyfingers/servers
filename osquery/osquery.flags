--config_path=osquery.conf
--config_refresh=10
--config_accelerated_refresh=300
--database_path=osquery.db
--tls_hostname=localhost:8889
--tls_server_certs=server.crt
--config_plugin=tls
--config_tls_endpoint=/config
--logger_plugin=tls
